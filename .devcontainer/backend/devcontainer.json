{
  "name": "who-does-what-backend",
  "dockerComposeFile": [
    "../../backend/docker-compose.yml"
  ],
  "service": "backend",
  "workspaceFolder": "/workspaces/who-does-what",
  "runServices": [
    "db"
  ],
  "postCreateCommand": "bash ./backend/devtools/install-dependencies.sh",
  "postStartCommand": "bash -lc \"bash ./backend/devtools/db/init-db.sh && cd backend && make db-upgrade\"",
  "shutdownAction": "stopCompose",
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
        "openai.chatgpt"
      ]
    }
  }
}
